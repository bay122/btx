@charset "utf-8";

body { font-family: Cambria, Georgia, "Arial Unicode MS", serif; font-size:=
 12pt; margin: 0px; padding: 0px; color: rgb(0, 0, 0); background: rgb(255,=
 255, 255); line-height: 1.35em; }

.no-print { display: none !important; }

h1, h2 { padding: 0px; margin: 0.35em 0px 0.2em; }

h1 { font-size: 150%; }

h2 { font-size: 130%; }

h3, h4 { padding: 0px; margin: 0.2em 0px; font-size: 115%; }

p { margin: 0.8em 0px; }

a { color: rgb(0, 0, 0); }

form { display: block; }

label { cursor: default; }

img { border: 0px; }

hr { background-color: rgb(174, 174, 174); color: rgb(174, 174, 174); borde=
r: 0px; height: 1px; }

#printContainerTop { margin-bottom: 15px; padding-bottom: 5px; border-botto=
m: 1px solid rgb(174, 174, 174); }

#printContainerBottom { margin-top: 15px; padding-top: 5px; border-top: 1px=
 solid rgb(174, 174, 174); text-align: right; }

table { border-collapse: collapse; }

tr { border-bottom: 1px dashed rgb(221, 221, 221); }

td { vertical-align: top; padding: 0px 3px; }

.rtl { direction: rtl; }

.copyright, .footnote_print { padding-top: 20px; font-size: 80%; direction:=
 ltr; }

.chapter, h3.chapter { line-height: 131%; }

.paragraph, .search .block .verse { padding-top: 10px; }

.verse { padding-left: 10px; text-indent: -10px; }

.rtl .verse { padding-left: 0px; padding-right: 10px; }

.verseNumber, .footnote { vertical-align: top; font-size: 75%; line-height:=
 150%; }

.verseNumber a { text-decoration: none; }

.noNumber .verse { display: inline; padding: 0px; text-indent: 0px; }

.noNumber .verseNumber { display: none; }

.search .highlight { font-style: italic; }

.footnote { font-style: italic; }

.noFootnote .footnote, .noFootnote .footnote_print { display: none; }

.note { margin-bottom: 5px; font-family: Verdana, Arial, sans-serif; backgr=
ound-color: rgb(221, 221, 221); font-size: 80%; }

.note_name { color: rgb(127, 124, 119); font-style: italic; }

.note_content { line-height: 100%; }

.small { font-size: 80%; }

.dijitDialog, .dijitPopup, .dijitTooltip, .dijitMenu, .dijitDialogUnderlayW=
rapper, .dijitDialogUnderlay { display: none; }

.clearfix::after, dl.list::after { content: "."; display: block; height: 0p=
x; clear: both; visibility: hidden; }

.clearfix, dl.list { display: inline-block; }

* html .clearfix, * html dl.list { height: 1%; }

.clearfix, dl.list { display: block; }
